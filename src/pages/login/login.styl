.login
  .top
    width: 100%
    height: 400px
    background-size: 100%
    background-repeat: no-repeat
    background-image: url("data:image/jpeg;base64,");
  .form
    width: 90%
    margin-left: 5%
    margin-top: -100px;
    border: 1px solid #eeeeee
    background-color: #fff
    .token
      margin: 20px;
      padding: 20px;
      border-bottom: 4px solid #80bd01
    .btn-login
      margin: 20px
      background-color: #80bd01 
      color: white
    .btn-wrapper
      display: flex
      align-items: center
      padding-bottom: 20px
      .qrcode,.github
        flex: 0 1 50%;
        text-align: center
        .iconfont
          font-size: 40px
  .tip
    text-align: center
    color: #80bd01  
    margin-top: 40px  
    text-decoration-line: underline
    cursor pointer

